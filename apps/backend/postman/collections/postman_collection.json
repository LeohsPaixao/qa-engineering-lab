        {
          "name": "Create User - Success",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "id": "6c24ea88-d174-4986-99d3-69723852d4f3",
                "exec": [
                  "function generateValidCpf() {",
                  "    const randomDigits = () => Math.floor(Math.random() * 9);",
                  "",
                  "    const cpf = [];",
                  "    for (let i = 0; i < 9; i++) {",
                  "        cpf.push(randomDigits());",
                  "    }",
                  "",
                  "    const calculateDigit = (base) => {",
                  "        let sum = 0;",
                  "        let weight = base.length + 1;",
                  "",
                  "        for (let i = 0; i < base.length; i++) {",
                  "            sum += base[i] * weight--;",
                  "        }",
                  "",
                  "        const mod = (sum * 10) % 11;",
                  "        return mod === 10 ? 0 : mod;",
                  "    };",
                  "",
                  "    cpf.push(calculateDigit(cpf));",
                  "    cpf.push(calculateDigit(cpf));",
                  "",
                  "    return cpf.join('');",
                  "}",
                  "",
                  "const cpf = generateValidCpf();",
                  "",
                  "pm.variables.set(\"full_name\", pm.variables.replaceIn(\"{{$randomFirstName}} {{$randomLastName}}\"));",
                  "pm.variables.set(\"social_name\", pm.variables.replaceIn(\"{{$randomFirstName}}\"));",
                  "pm.variables.set(\"document\", pm.variables.replaceIn(cpf));",
                  "pm.variables.set(\"phone\", pm.variables.replaceIn(\"{{$randomPhoneNumber}}\"));",
                  "pm.variables.set(\"email\", pm.variables.replaceIn(\"{{$randomEmail}}\"));",
                  "pm.variables.set(\"password\", pm.variables.replaceIn(\"123456\"));",
                  ""
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
            {
              "listen": "test",
              "script": {
                "id": "7f591d39-241c-46c5-b556-1cd1ec924086",
                "exec": [
                  "pm.test(\"Should be able to create a new user\", function () {",
                  "    pm.expect(pm.response.code).to.eql(201)",
                  "    pm.expect(pm.response.json().message).to.eql(\"Usuário criado com sucesso!\")",
                  "})"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
