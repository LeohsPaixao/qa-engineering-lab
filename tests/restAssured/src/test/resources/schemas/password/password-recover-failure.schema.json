{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "The Password Recover Failure Schema",
  "required": [
    "message",
    "error",
    "statusCode"
  ],
  "properties": {
    "message": {
      "$id": "#/properties/message",
      "type": "string",
      "title": "The Message Schema",
      "examples": [
        "Usuário não encontrado."
      ]
    },
    "error": {
      "$id": "#/properties/error",
      "type": "string",
      "title": "The Error Schema",
      "examples": [
        "Not Found"
      ]
    },
    "statusCode": {
      "$id": "#/properties/statusCode",
      "type": "number",
      "title": "The Status Code Schema",
      "examples": [
        "404"
      ]
    }
  }
}